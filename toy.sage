from sage.groups.generic import discrete_log_lambda
import time

p = Integer()

g = Mod(
    ,
    p
)

# Toy nonce
bits = 20
r_test = randint(0, 2^bits - 1)
a_test = g^r_test

start = time.time()
r = discrete_log_lambda(a_test, g, (0, 2^bits))
end = time.time()

print("Recovered r:", r)
print("Correct:", r == r_test)
print("Time:", end - start, "seconds")
